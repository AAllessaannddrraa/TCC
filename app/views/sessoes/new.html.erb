<h1>Entrar</h1>
<%= form_with(url: login_path, scope: :session, local: true) do |form| %>
  <div class="field">
    <%= form.label :email %>
    <%= form.text_field :email, class: 'form-control' %>
  </div>

  <div class="field">
    <%= form.label :senha %>
    <%= form.password_field :senha, class: 'form-control' %>
  </div>

  <div class="actions">
    <%= form.submit "Entrar", class: "btn btn-primary" %>
  </div>
<% end %>
